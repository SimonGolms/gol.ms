{
  "name": "gol.ms-website",
  "version": "1.1.8",
  "description": "The personal website from Simon Golms",
  "private": true,
  "homepage": "https://gol.ms",
  "license": "MIT",
  "author": {
    "name": "Simon Golms",
    "email": "development@gol.ms",
    "url": "https://gol.ms"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simongolms/gol.ms"
  },
  "bugs": {
    "url": "https://github.com/simongolms/gol.ms/issues"
  },
  "scripts": {
    "build-stats": "ANALYZE=true npm run build",
    "build": "next build",
    "dev": "next --port 8080",
    "export": "next export -o dist",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "release:ci": "semantic-release",
    "repair": "npx rimraf .git/hooks .next dist node_modules package-lock.json && npm install --force",
    "start": "next start"
  },
  "dependencies": {
    "next": "12.1.6",
    "next-seo": "5.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@next/bundle-analyzer": "12.1.6",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "autoprefixer": "10.4.7",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-no-type-assertion": "1.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.2",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "semantic-release": "19.0.3",
    "tailwindcss": "3.1.3",
    "typescript": "4.7.4"
  }
}
